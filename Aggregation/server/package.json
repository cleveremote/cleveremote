{
  "name": "mean-auth-boilerplate-server",
  "version": "1.0.0",
  "description": "Minimal boilerplate for mean architecture using ts",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug=0.0.0.0:5200 --watch",
    "start:debug1": "nest start --debug=0.0.0.0:5900 --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "watch-ts": "tsc -w",
    "typeorm:cli": "ts-node node_modules/typeorm/cli -f src/common/typeorm.config.ts"
  },
  "author": "",
  "license": "ISC",
